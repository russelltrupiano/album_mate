= provide(:title, "Edit Profile")
.row
  .col-4.col-offset-4
    %h3 Update your profile
    = form_for(@user) do |f|
      = render "fields", f: f
      .form-field.submit-wrapper.row
        = f.submit "Update", class: "mdl-button mdl-js-button mdl-button--raised mdl-js-ripple-effect mdl-button--accent pad-small-button"